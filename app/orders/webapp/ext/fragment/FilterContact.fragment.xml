<core:FragmentDefinition xmlns:core="sap.ui.core" xmlns="sap.m">
        <ComboBox
            id="Contact"
            selectedKey="{path: 'filterValues>', type: 'sap.fe.macros.filter.type.Value'}"
            items="{path: '/Contacts'}"
        >
            <items>
                <core:Item key="{ID}" text="{firstName} {lastName}"/>
            </items>
        </ComboBox>
</core:FragmentDefinition>
